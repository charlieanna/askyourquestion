<ul class="questions" ng-controller="QuestionsCtrl" ng-init="get('<%= event.id%>');subscribe_as_admin('<%=event.code%>',<%= current_admin.present?%>)">
  <li class="row question animate-repeat question" ng-repeat="question in questions | orderBy:predicate:reverse"  class="animate-repeat">
     <% unless admin_signed_in? %>
       <span class="vote-buttons">
      <button ng-click="up(question)" class="btn btn-success" id="bluebutton" ng-show="!question.liked" style="width:8%"><i class='fa fa-thumbs-up'></i>Up
      </button>
  
      <button class="btn btn-success" ng-click="down(question)" ng-show="question.liked" style="width:8%"><i class='fa fa-thumbs-down'></i>Down</button>
    </span>
      <% else %>
      <span class="vote-buttons">
        <button class="btn btn-success" id="bluebutton" ng-click="approve(question)"><i class='fa fa-thumbs-up'></i>Approve </button>
        <button class="btn btn-success"  ng-click="reject(question)" id="bluebutton"><i class='fa fa-thumbs-down'></i>Reject</button>
    
   </span>
     
       
      <% end %>
    
      <span class="body">
      {{question.body }} </span>
      <span class="votes">{{question.votes}}</span>
    
   
  </li>
  <!-- <form ng-submit="addQuestion()">
    <input type="text" ng-model="question.body" placeholder="Enter a question here">
    <input type="text" ng-model="question.votes" placeholder="Enter votes here">
    <input type="submit" value="Test">
  </form> -->
</ul>
